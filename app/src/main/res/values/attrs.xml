<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- JZCalendar属性 -->
    <declare-styleable name="JZCalendar">
        <!-- 月份条背景 -->
        <attr name="monthBarBackground" format="color"/>
        <!-- 月份条字体颜色 -->
        <attr name="monthBarTextColor" format="color"/>
        <!-- 月份条字体大小 -->
        <attr name="monthBarTextSize" format="dimension"/>
        <!-- 日历区域背景 -->
        <attr name="dateBackground" format="color"/>
        <!-- 周始周末字体颜色 -->
        <attr name="weekendTextColor" format="color"/>
        <!-- 日期字体颜色 -->
        <attr name="dateTextColor" format="color"/>
        <!-- 日期字体大小 -->
        <attr name="dateTextSize" format="dimension"/>
        <!-- 其他月份字体颜色 -->
        <attr name="otherTextColor" format="color"/>
        <!-- 今日字体颜色 -->
        <attr name="todayTextColor" format="color"/>
        <!-- 选中日期字体颜色 -->
        <attr name="selectTextColor" format="color"/>
    </declare-styleable>
</resources>